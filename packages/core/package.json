{
    "name": "@path-logic/core",
    "version": "0.1.0",
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src && prettier --check . --ignore-path ../../.prettierignore --ignore-unknown",
        "docs": "typedoc --options typedoc.json"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "vitest": "^1.0.0",
        "typedoc": "^0.25.0"
    }
}
